#include <math.h>
#include <stdio.h>

int main()
 {

     float i,j,x,y; // første løkke
     float pi = M_PI;

     y = pi/30;



     for (i=0;(y*i)<=pi;i++) // denne for løkken vil kjøre så mangen ganger som b er.
     {
         x = sin(y)*30;
        for (j=0;j<=x;j++) // denne løkken printer ut *, antall stjerner blir bestemt av a.
         {
             printf("*");
         }
         printf("\n");
     }
     return 0;
 }
